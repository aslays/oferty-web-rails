<% if current_user.rol==1 %>

<p id="notice"><%= notice %></p>
<div class="show">
<p class= "bg-primary">
  <strong>Categoria:</strong>
  <%= @categorium.categoria %>
</p>

<%= link_to 'Editar', edit_categorium_path(@categorium), class: "btn btn-warning" %> 
<%= link_to 'Volver', categoria_path, class: "btn btn-success"%>
</div>
<% else %>
        <p>Debes iniciar sesion <%=link_to 'Login', usuario_sessions_new_path %></p>
<% end %> 